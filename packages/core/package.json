{
  "name": "@mobx-state-machine-router/core",
  "version": "5.0.1",
  "description": "State Machine routing for MobX",
  "author": "Anzor Bashkhaz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anzorb/mobx-state-machine-router.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "jest ."
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "mobx": "^4.15.7",
    "mobx-async-intercept": "^1.1.10",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "mobx": "^4.15.7 || ^5.0.0 || ^6.0.0"
  },
  "keywords": [
    "mobx",
    "state-machine",
    "router",
    "state-management"
  ]
}
