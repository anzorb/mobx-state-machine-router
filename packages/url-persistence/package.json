{
  "name": "@mobx-state-machine-router/url-persistence",
  "version": "6.0.0",
  "description": "URL persistence layer for MobX State Machine Router",
  "author": "Anzor Bashkhaz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anzorb/mobx-state-machine-router.git"
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "jest ."
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "history": "^5.3.0",
    "query-string": "^6.14.1"
  },
  "devDependencies": {
    "@mobx-state-machine-router/core": "workspace:*",
    "mobx": "^4.15.7",
    "tsup": "^8.4.0"
  },
  "peerDependencies": {
    "history": "^5.0.0",
    "mobx": "^4.15.7 || ^5.0.0 || ^6.0.0"
  },
  "keywords": [
    "mobx",
    "state-machine",
    "router",
    "url",
    "persistence"
  ]
}
